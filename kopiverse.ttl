@prefix : <http://kopiverse.org/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://kopiverse.org/ontology#> .

<http://kopiverse.org/ontology> rdf:type owl:Ontology ;
                                 rdfs:comment "KopiVerse - Semantic Project (Full Commercial Data)."@id .

#################################################################
#    Object Properties
#################################################################

:brewedFrom rdf:type owl:ObjectProperty ;
            rdfs:domain :CoffeeBeverage ;
            rdfs:range :CoffeeProduct .

:hasDerivedOrigin rdf:type owl:ObjectProperty ;
                  owl:propertyChainAxiom ( :producedBy :isLocatedIn ) .

:hasFlavorNote rdf:type owl:ObjectProperty ;
               rdfs:range :Descriptor .

:hasIngredient rdf:type owl:ObjectProperty ;
               rdfs:range :Ingredient .

:hasVarietal rdf:type owl:ObjectProperty ;
             rdfs:domain :CoffeeLot , :CoffeeProduct ;
             rdfs:range :CoffeePlant .

:isLocatedIn rdf:type owl:ObjectProperty , owl:TransitiveProperty ;
             rdfs:range :GeographicRegion .

:processedWith rdf:type owl:ObjectProperty ;
               rdfs:domain :CoffeeLot ;
               rdfs:range :ProcessingMethod ;
               rdfs:label "diproses dengan"@id .

:producedBy rdf:type owl:ObjectProperty ;
            owl:inverseOf :produces ;
            rdfs:domain :CoffeeLot , :CoffeeProduct ;
            rdfs:range :Actor , :Farm .

:produces rdf:type owl:ObjectProperty ;
          rdfs:domain :Actor , :Farm ;
          rdfs:range :CoffeeLot , :CoffeeProduct .

:servedBy rdf:type owl:ObjectProperty ;
          rdfs:domain :CoffeeBeverage ;
          rdfs:range :Actor , :Coffeeshop .

:hasCertification rdf:type owl:ObjectProperty ;
                  rdfs:range :Certification .

:isRoastedFrom rdf:type owl:AnnotationProperty .

#################################################################
#    Data Properties (Lengkap untuk semua Aktor)
#################################################################

# --- General ---
:hasAltitude rdf:type owl:DatatypeProperty ; rdfs:range xsd:integer .
:hasCuppingScore rdf:type owl:DatatypeProperty ; rdfs:range xsd:decimal .
:hasPrice rdf:type owl:DatatypeProperty ; rdfs:range xsd:decimal .

# --- Petani ---
:hasHarvestDate rdf:type owl:DatatypeProperty ; rdfs:range xsd:string .
:isOrganic rdf:type owl:DatatypeProperty ; rdfs:range xsd:boolean .
:hasFertilizerType rdf:type owl:DatatypeProperty ; rdfs:range xsd:string .

# --- Roaster (Green Bean QC) ---
:hasMoistureContent rdf:type owl:DatatypeProperty ; rdfs:range xsd:decimal .
:hasBeanDensity rdf:type owl:DatatypeProperty ; rdfs:range xsd:string .
:hasScreenSize rdf:type owl:DatatypeProperty ; rdfs:range xsd:string .
:hasWaterActivity rdf:type owl:DatatypeProperty ; rdfs:range xsd:decimal .
:hasDefectCount rdf:type owl:DatatypeProperty ; rdfs:range xsd:integer .
:hasBeanGrade rdf:type owl:DatatypeProperty ; rdfs:range xsd:string .
:hasCropYear rdf:type owl:DatatypeProperty ; rdfs:range xsd:string .
:hasStockKg rdf:type owl:DatatypeProperty ; rdfs:range xsd:integer .
:packagingType rdf:type owl:DatatypeProperty ; rdfs:range xsd:string .

# --- Roaster (Profiling) ---
:recommendedRoastProfile rdf:type owl:DatatypeProperty ; rdfs:range xsd:string .
:hasAgtronNumber rdf:type owl:DatatypeProperty ; rdfs:range xsd:integer .
:hasFirstCrackTemp rdf:type owl:DatatypeProperty ; rdfs:range xsd:integer .
:recommendedDTR rdf:type owl:DatatypeProperty ; rdfs:range xsd:string .
:hasShrinkage rdf:type owl:DatatypeProperty ; rdfs:range xsd:decimal .
:restingDays rdf:type owl:DatatypeProperty ; rdfs:range xsd:integer .

# --- Sensory & Process ---
:fermentationHours rdf:type owl:DatatypeProperty ; rdfs:range xsd:integer .
:acidityLevel rdf:type owl:DatatypeProperty ; rdfs:range xsd:string .
:bodyLevel rdf:type owl:DatatypeProperty ; rdfs:range xsd:string .

# --- Barista (Brewing) ---
:brewingTemp rdf:type owl:DatatypeProperty ; rdfs:range xsd:integer .
:brewingTime rdf:type owl:DatatypeProperty ; rdfs:range xsd:string .
:waterRatio rdf:type owl:DatatypeProperty ; rdfs:range xsd:string .

#################################################################
#    Classes
#################################################################

:Actor rdf:type owl:Class .
:CoffeeBeverage rdf:type owl:Class .
:CoffeeLot rdf:type owl:Class ; rdfs:subClassOf :CoffeeProduct .
:CoffeeProduct rdf:type owl:Class .
:CoffeePlant rdf:type owl:Class .
:Coffeeshop rdf:type owl:Class ; rdfs:subClassOf :Actor .
:Farm rdf:type owl:Class ; rdfs:subClassOf :Actor .
:GeographicRegion rdf:type owl:Class .
:Ingredient rdf:type owl:Class .
:ProcessingMethod rdf:type owl:Class .
:Descriptor rdf:type owl:Class .
:Certification rdf:type owl:Class .
:RoastedCoffee rdf:type owl:Class ; rdfs:subClassOf :CoffeeProduct .

# Special Classes
:SpecialtyCoffee rdf:type owl:Class .
:PremiumLot rdf:type owl:Class .
:HighAltitudeFarm rdf:type owl:Class .

